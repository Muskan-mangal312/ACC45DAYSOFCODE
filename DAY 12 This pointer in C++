#include<iostream>
#include<string.h>
using namespace std;
class student
{
    int roll_no,sec;
    string name;
    public:
    student()
    {
        roll_no = sec = 0;
        name = " ";
    }
    student(int roll_no,int sec,string name)
    {
        this->roll_no = roll_no;
        this->sec = sec;
        this->name = name;
    }
    
    void disp(){
        cout<<roll_no<<" "<<sec<<" "<<name;
    }
};
int main(){
    student ob1(12,3,"aman");
   
    ob1.disp();

return 0;
}
